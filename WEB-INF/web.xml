<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ConfigAuditServlet</servlet-name>
        <servlet-class>com.finlogic.apps.finstudio.controller.ConfigAuditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeploynRestartTomcatServlet</servlet-name>
        <servlet-class>com.finlogic.apps.chatbot.finws.DeploynRestartTomcatServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConfigAuditServlet</servlet-name>
        <url-pattern>/configaudit.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.fin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MultiFileUploadServlet</servlet-name>
        <servlet-class>com.finlogic.apps.finstudio.webservice.MultiFileUploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MultiFileUploadServlet</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeploynRestartTomcatServlet</servlet-name>
        <url-pattern>/DeploynRestartTomcatServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>FinFileTransferServlet</servlet-name>
        <!--<servlet-class>uploadSelFile</servlet-class>-->
        <servlet-class>com.finlogic.util.finfiletransfer.FinFileTransferServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FinFileTransferServlet</servlet-name>
        <!--<url-pattern>/upload.htm</url-pattern>-->
        <url-pattern>/FinFileTransferServlet.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetProjectFileServlet</servlet-name>
        <servlet-class>com.finlogic.apps.finstudio.filedirautorequest.GetProjectFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetProjectFileServlet</servlet-name>
        <url-pattern>/getprojectfile.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetProjectFileListServlet</servlet-name>
        <servlet-class>com.finlogic.apps.finstudio.filedirautorequest.GetProjectFileListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetProjectFileListServlet</servlet-name>
        <url-pattern>/getprojectfilelist.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetEncTextServlet</servlet-name>
        <servlet-class>com.finlogic.finstudio.encdec.util.GetEncTextServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetEncTextServlet</servlet-name>
        <url-pattern>/getenctext.do</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>FinstudioFilter</filter-name>
        <filter-class>com.finlogic.util.filter.FinstudioFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FinstudioFilter</filter-name>
        <url-pattern>/generated/*</url-pattern>
    </filter-mapping>
</web-app>
